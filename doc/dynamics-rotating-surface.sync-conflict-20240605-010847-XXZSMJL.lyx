\begin_layout Subsection
Уравнения движения в полярных координатах
\end_layout

\begin_layout Standard
Вводим новые координаты 
\begin_inset Formula 
\begin{align*}
\rho & =\sqrt{x^{2}+y^{2}}\\
\phi & =\arctan\frac{y}{x}\\
x & =\rho\cos\phi\\
y & =\rho\sin\phi
\end{align*}

\end_inset

Вычисляем производные 
\begin_inset Formula 
\begin{align*}
\dot{\rho} & =\cos\phi\dot{x}+\sin\phi\dot{y}\\
\dot{\phi} & =\frac{1}{1+\left(\frac{y}{x}\right)^{2}}\frac{\dot{y}x-y\dot{x}}{x^{2}}\\
 & =\frac{\dot{y}\rho\cos\phi-\rho\sin\phi\dot{x}}{\rho^{2}}\\
 & =\frac{-\sin\phi\dot{x}+\cos\phi\dot{y}}{\rho}\\
\frac{d}{dt}\left(\begin{array}{c}
\rho\\
\phi
\end{array}\right) & =\left(\begin{array}{cc}
1 & 0\\
0 & \frac{1}{\rho}
\end{array}\right)\left(\begin{array}{cc}
\cos\phi & \sin\phi\\
-\sin\phi & \cos\phi
\end{array}\right)\left(\begin{array}{c}
\dot{x}\\
\dot{y}
\end{array}\right)\\
 & =\left(\begin{array}{cc}
1 & 0\\
0 & \frac{1}{\rho}
\end{array}\right)R_{\phi}^{T}\frac{d}{dt}\left(\begin{array}{c}
x\\
y
\end{array}\right)\\
\frac{d}{dt}\left(\begin{array}{c}
x\\
y
\end{array}\right) & =R_{\phi}\left(\begin{array}{cc}
1 & 0\\
0 & \rho
\end{array}\right)\frac{d}{dt}\left(\begin{array}{c}
\rho\\
\phi
\end{array}\right)
\end{align*}

\end_inset

Подставляем в уравнения динамики
\begin_inset Formula 
\begin{align*}
\gamma & =-\mu\times\omega-\dot{\mu}\\
a & =-2\mu\times v-\left(\mu^{\wedge}\mu^{\wedge}+\dot{\mu}^{\wedge}\right)p\\
\lambda & =m\left[I-\frac{mr^{2}}{M}n^{\wedge}n^{\wedge}\right]^{-1}\left[gR_{tw}\mathbf{e}_{z}+r\omega\times\dot{n}+r\gamma\times n-a\right]\\
v & =r\omega\times n\\
\dot{\omega} & =\gamma+\frac{1}{M}r\lambda\times n\\
\dot{v} & =-gR_{tw}\mathbf{e}_{z}+a+\frac{1}{m}\lambda.
\end{align*}

\end_inset

Фиг знает как получить...
 Но есть такое тождество (при условии 
\begin_inset Formula $\left\Vert n\right\Vert =1$
\end_inset

)
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n^{\wedge}\left[I-\frac{mr^{2}}{M}n^{\wedge}n^{\wedge}\right]^{-1}=\frac{1}{\frac{mr^{2}}{M}+1}n^{\wedge}.
\]

\end_inset

Используем 
\begin_inset Formula 
\[
v=r\omega\times n
\]

\end_inset

Тогда 
\begin_inset Formula 
\begin{align*}
\dot{\omega} & =\gamma-\frac{rm}{M}n^{\wedge}\left[I-\frac{mr^{2}}{M}n^{\wedge}n^{\wedge}\right]^{-1}\left[gR_{tw}\mathbf{e}_{z}+r\omega\times\dot{n}+r\gamma\times n-a\right]\\
 & =\gamma-\frac{rm}{mr^{2}+M}n^{\wedge}\left[gR_{tw}\mathbf{e}_{z}+r\omega\times\dot{n}+r\gamma\times n-a\right]\\
 & =\gamma-\frac{mr}{mr^{2}+M}n^{\wedge}\left[gR_{tw}\mathbf{e}_{z}-r\dot{n}\times\omega-rn\times\gamma-a\right]\\
 & =\gamma-\frac{k}{r}n^{\wedge}\left[gR_{tw}\mathbf{e}_{z}-r\dot{n}\times\omega-rn\times\gamma-a\right]\\
 & =-\mu\times\omega-\dot{\mu}-\frac{k}{r}n^{\wedge}\left[gR_{tw}\mathbf{e}_{z}-r\dot{n}^{\wedge}\omega-rn^{\wedge}\left(-\mu\times\omega-\dot{\mu}\right)-2r\mu^{\wedge}n^{\wedge}\omega+\left(\mu^{\wedge}\mu^{\wedge}+\dot{\mu}^{\wedge}\right)p\right]\\
 & =-\mu^{\wedge}\omega-\dot{\mu}-\frac{k}{r}n^{\wedge}\left[gR_{tw}\mathbf{e}_{z}-r\dot{n}^{\wedge}\omega+r\left(n^{\wedge}\mu^{\wedge}\omega-2\mu^{\wedge}n^{\wedge}\omega\right)+rn^{\wedge}\dot{\mu}+\left(\mu^{\wedge}\mu^{\wedge}+\dot{\mu}^{\wedge}\right)p\right]\\
 & =-\mu^{\wedge}\omega-\dot{\mu}-\left[\frac{kg}{r}n^{\wedge}R_{tw}\mathbf{e}_{z}-\frac{k}{r}n^{\wedge}r\dot{n}^{\wedge}\omega+\frac{k}{r}n^{\wedge}r\left(n^{\wedge}\mu^{\wedge}\omega-2\mu^{\wedge}n^{\wedge}\omega\right)+\frac{k}{r}n^{\wedge}rn^{\wedge}\dot{\mu}+\frac{k}{r}n^{\wedge}\left(\mu^{\wedge}\mu^{\wedge}+\dot{\mu}^{\wedge}\right)p\right]\\
 & =\left[-\frac{kg}{r}n^{\wedge}R_{tw}\mathbf{e}_{z}\right]+\left[-\frac{k}{r}n^{\wedge}\mu^{\wedge}\mu^{\wedge}p\right]+\left[-\mu^{\wedge}+kn^{\wedge}\dot{n}^{\wedge}-kn^{\wedge}n^{\wedge}\mu^{\wedge}+2kn^{\wedge}\mu^{\wedge}n^{\wedge}\right]\omega+\left[\frac{k}{r}n^{\wedge}p^{\wedge}-I-kn^{\wedge}n^{\wedge}\right]\dot{\mu}
\end{align*}

\end_inset


\end_layout

