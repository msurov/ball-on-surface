Находим производные
\begin_inset Formula 
\begin{align*}
\sigma & =\rho\left(\begin{array}{c}
\cos\phi\\
\sin\phi\\
\tan\alpha
\end{array}\right)\\
\sigma_{\phi} & =\rho\left(\begin{array}{c}
-\sin\phi\\
\cos\phi\\
0
\end{array}\right)\\
\sigma_{\rho} & =\left(\begin{array}{c}
\cos\phi\\
\sin\phi\\
\tan\alpha
\end{array}\right)\\
\sigma_{\rho}\times\sigma_{\phi} & =\rho\left(\begin{array}{c}
\cos\phi\\
\sin\phi\\
\tan\alpha
\end{array}\right)\times\left(\begin{array}{c}
-\sin\phi\\
\cos\phi\\
0
\end{array}\right)=\rho\left(\begin{array}{c}
-\tan\alpha\cos\phi\\
-\tan\alpha\sin\phi\\
1
\end{array}\right)\\
n & =\frac{\sigma_{\rho}\times\sigma_{\phi}}{\left\Vert \sigma_{\rho}\times\sigma_{\phi}\right\Vert }=\left(\begin{array}{c}
-\sin\alpha\cos\phi\\
-\sin\alpha\sin\phi\\
\cos\alpha
\end{array}\right)\\
\dot{n} & =\left(\begin{array}{c}
\sin\alpha\sin\phi\\
-\sin\alpha\cos\phi\\
0
\end{array}\right)\dot{\phi}
\end{align*}

\end_inset

Матрица преобразования
\begin_inset Formula 
\begin{align*}
\zeta & =A\left(\phi,\rho\right)\omega\\
 & =\left(\begin{array}{c}
\sigma_{\rho}\\
\sigma_{\phi}\\
n
\end{array}\right)\omega\\
 & =\left(\begin{array}{ccc}
\cos\phi\cos\alpha & \sin\phi\cos\alpha & \sin\alpha\\
-\sin\phi & \cos\phi & 0\\
-\cos\phi\sin\alpha & -\sin\phi\sin\alpha & \cos\alpha
\end{array}\right)\omega\\
 & =\mathbf{R}_{\mathbf{e}_{y},\alpha}\mathbf{R}_{\mathbf{e}_{z},-\phi}\omega\\
\mathbf{R}_{\mathbf{e}_{z},\phi}\mathbf{R}_{\mathbf{e}_{y},-\alpha}\zeta & =\omega
\end{align*}

\end_inset

Динамика в новых координатах 
\begin_inset Formula 
\begin{align*}
\frac{dA}{d\phi} & =\frac{d\mathbf{R}_{\mathbf{e}_{y},\alpha}\mathbf{R}_{\mathbf{e}_{z},-\phi}}{d\phi}\\
 & =-A\mathbf{e}_{z}^{\wedge}\\
\frac{dA}{d\phi}A^{T} & =-A\mathbf{e}_{z}^{\wedge}A^{T}=-\left(A\mathbf{e}_{z}\right)^{\wedge}
\end{align*}

\end_inset

Подставляем
\begin_inset Formula 
\begin{align*}
\dot{\zeta} & =\dot{A}\omega+A\dot{\omega}\\
 & =\dot{A}A^{T}\zeta+A\dot{\omega}\\
 & =-\left(A\mathbf{e}_{z}\right)^{\wedge}\dot{\phi}\zeta+A\dot{\omega}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\dot{\omega} & =\left[-\frac{kg}{r}n^{\wedge}\mathbf{e}_{z}\right]+\left[\frac{k}{r}n^{\wedge}D_{1,1,0}p\right]\dot{\theta}^{2}+\left[-\dot{\theta}\mathbf{e}_{z}^{\wedge}+kn^{\wedge}\dot{n}^{\wedge}-k\dot{\theta}n^{\wedge}n^{\wedge}\mathbf{e}_{z}^{\wedge}+2k\dot{\theta}n^{\wedge}\mathbf{e}_{z}^{\wedge}n^{\wedge}\right]\omega+\left[\frac{k}{r}n^{\wedge}p^{\wedge}-I-kn^{\wedge}n^{\wedge}\right]\mathbf{e}_{z}\ddot{\theta}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $-\dot{\phi}\left(A\mathbf{e}_{z}\right)^{\wedge}=\dot{\phi}\left(\begin{array}{ccc}
0 & \cos\alpha & 0\\
-\cos\alpha & 0 & \sin\alpha\\
0 & -\sin\alpha & 0
\end{array}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $A\left[-\frac{kg}{r}n^{\wedge}\mathbf{e}_{z}\right]=-\frac{gk}{r}\sin\alpha\mathbf{e}_{y}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $A\left[\frac{k}{r}n^{\wedge}D_{1,1,0}p\right]=\rho\frac{k}{r}\cos\alpha\mathbf{e}_{y}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $A\left[-\dot{\theta}\mathbf{e}_{z}^{\wedge}-k\dot{\theta}n^{\wedge}n^{\wedge}\mathbf{e}_{z}^{\wedge}+2k\dot{\theta}n^{\wedge}\mathbf{e}_{z}^{\wedge}n^{\wedge}\right]A^{T}=\left(\begin{array}{ccc}
0 & \left(k+1\right)\cos\alpha & 0\\
-\left(k+1\right)\cos\alpha & 0 & \left(1-k\right)\sin\alpha\\
0 & -\sin\alpha & 0
\end{array}\right)\dot{\theta}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $A\left[kn^{\wedge}\dot{n}^{\wedge}\right]A^{T}=-k\sin\alpha\dot{\phi}\left(\begin{array}{ccc}
0 & 0 & 0\\
0 & 0 & 1\\
0 & 0 & 0
\end{array}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $A\left[\frac{k}{r}n^{\wedge}p^{\wedge}-I-kn^{\wedge}n^{\wedge}\right]\mathbf{e}_{z}=\left(\begin{array}{c}
k\sin\alpha-\sin\alpha\\
0\\
-\cos\alpha
\end{array}\right)+\mathbf{e}_{x}\frac{k\rho}{r}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\dot{\zeta} & =\underbrace{\left[-\frac{gk}{r}\sin\alpha\mathbf{e}_{y}\right]}_{a_{1}}+\underbrace{\left[\rho\frac{k}{r}\cos\alpha\mathbf{e}_{y}\right]}_{\rho a_{2}}\dot{\theta}^{2}+\underbrace{\dot{\theta}\left[\begin{array}{ccc}
0 & \left(k+1\right)\cos\alpha & 0\\
-\left(k+1\right)\cos\alpha & 0 & \left(1-k\right)\sin\alpha\\
0 & -\sin\alpha & 0
\end{array}\right]}_{\dot{\theta}A_{3}}\zeta\\
 & +\underbrace{\dot{\phi}\left[\begin{array}{ccc}
0 & \cos\alpha & 0\\
-\cos\alpha & 0 & \left(1-k\right)\sin\alpha\\
0 & -\sin\alpha & 0
\end{array}\right]}_{\dot{\phi}A_{4}}\zeta+\left[\underbrace{\left(\begin{array}{c}
\left(k-1\right)\sin\alpha\\
0\\
-\cos\alpha
\end{array}\right)}_{a_{5}}+\underbrace{\mathbf{e}_{x}\frac{k\rho}{r}}_{\rho a_{6}}\right]\ddot{\theta}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\rho & =\sqrt{x^{2}+y^{2}}\\
\phi & =\arctan\frac{y}{x}\\
\dot{\rho} & =\cos\phi\dot{x}+\sin\phi\dot{y}\\
\dot{\phi} & =\frac{\dot{y}\cos\phi-\sin\phi\dot{x}}{\rho}\\
\left(\begin{array}{c}
\dot{\rho}\\
\dot{\phi}
\end{array}\right) & =\left(\begin{array}{cc}
\cos\phi & \sin\phi\\
-\frac{1}{\rho}\sin\phi & \frac{1}{\rho}\cos\phi
\end{array}\right)\left(\begin{array}{c}
\dot{x}\\
\dot{y}
\end{array}\right)\\
v & =-rn^{\wedge}A^{T}\zeta\\
\left(\begin{array}{c}
\dot{\rho}\\
\dot{\phi}
\end{array}\right) & =r\left(\begin{array}{ccc}
0 & \cos\alpha & 0\\
-\frac{1}{\rho} & 0 & 0
\end{array}\right)\zeta
\end{align*}

\end_inset

В итоге 
\begin_inset Formula 
\begin{align*}
\dot{\rho} & =r\cos\alpha\zeta_{\phi}\\
\dot{\phi} & =-\frac{r}{\rho}\zeta_{\rho}\\
\dot{\zeta}_{\rho} & =\dot{\theta}\left(k+1\right)\cos\alpha\zeta_{\phi}+\dot{\phi}\cos\alpha\zeta_{\phi}+\left(k-1\right)\sin\alpha\ddot{\theta}+\frac{k\rho}{r}\ddot{\theta}\\
\dot{\zeta}_{\phi} & =-\frac{gk}{r}\sin\alpha+\rho\frac{k}{r}\cos\alpha\dot{\theta}^{2}-\cos\alpha\zeta_{\rho}\left(k\dot{\theta}+\dot{\theta}+\dot{\phi}\right)+\left(1-k\right)\sin\alpha\zeta_{n}\left(\dot{\theta}+\dot{\phi}\right)\\
\dot{\zeta}_{n} & =-\sin\alpha\zeta_{\phi}\left(\dot{\theta}+\dot{\phi}\right)-\cos\alpha\ddot{\theta}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
Анализ траекторий
\end_layout

\begin_layout Standard
Существование крговых траекторий.
 Предположим 
\begin_inset Formula $\rho=\text{const}$
\end_inset

, тогда 
\begin_inset Formula 
\begin{align*}
\zeta_{\phi} & =0\\
\zeta_{n} & =0\\
\zeta_{\rho} & =\text{const}
\end{align*}

\end_inset

тогда
\begin_inset Formula 
\begin{align*}
0 & =-\frac{gk}{r}\sin\alpha+\rho\frac{k}{r}\cos\alpha\dot{\theta}^{2}-\dot{\theta}\left(k+1\right)\cos\alpha\zeta_{\rho}+\frac{r}{\rho}\cos\alpha\zeta_{\rho}^{2}
\end{align*}

\end_inset

вычисляем 
\begin_inset Formula 
\begin{align*}
\zeta_{\rho}^{2}-\frac{\rho}{r}\dot{\theta}\left(k+1\right)\zeta_{\rho}-\frac{\rho gk}{r^{2}}\tan\alpha+\frac{\rho^{2}k}{r^{2}}\dot{\theta}^{2} & =0
\end{align*}

\end_inset

решаем квадратное уравнение: 
\begin_inset Formula 
\[
\zeta_{\rho}=\frac{\rho\dot{\theta}\left(k+1\right)\pm\sqrt{\rho^{2}\dot{\theta}^{2}\left(k-1\right)^{2}+4\rho gk\tan\alpha}}{2r}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\rho\dot{\theta}^{2}>-\frac{4gk\tan\alpha}{\left(k-1\right)^{2}}
\]

\end_inset

Как найти период? 
\begin_inset Formula 
\begin{align*}
T & =\frac{2\pi\rho}{r\zeta_{\rho}}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
Система управления
\end_layout

\begin_layout Standard
Динамика
\begin_inset Formula 
\begin{align*}
\dot{\zeta} & =a_{1}+\rho a_{2}\dot{\theta}^{2}+\dot{\theta}A_{3}\zeta+\dot{\phi}A_{4}\zeta+\left[a_{5}+\rho a_{6}\right]\ddot{\theta}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $x=\left(\delta\dot{\theta},\delta\rho,\delta\zeta\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\dot{\phi}=-\frac{r}{\rho}\zeta_{\rho}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\dot{\zeta}=a_{1}+\left(\rho_{*}+\delta\rho\right)a_{2}\left(\dot{\theta}_{*}+\delta\dot{\theta}\right)^{2}+\dot{\theta}A_{3}\left(\zeta_{*}+\delta\zeta\right)+-\frac{r}{\rho}\left(\zeta_{\rho*}+\delta\zeta_{\rho}\right)A_{4}\left(\zeta_{*}+\delta\zeta\right)+\left[a_{5}+\rho a_{6}\right]\ddot{\theta}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\dot{\zeta}=a_{1}+\left(\rho_{*}+\delta\rho\right)a_{2}\left(\dot{\theta}_{*}+\delta\dot{\theta}\right)^{2}+\dot{\theta}A_{3}\left(\zeta_{*}+\delta\zeta\right)+-\frac{r}{\rho}\left(\zeta_{\rho*}+\delta\zeta_{\rho}\right)A_{4}\left(\zeta_{*}+\delta\zeta\right)+\left[a_{5}+\rho_{*}a_{6}\right]\ddot{\theta}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\dot{\zeta}=a_{1}+\left(\rho_{*}\right)a_{2}\left(2\dot{\theta}_{*}\delta\dot{\theta}\right)+\left(\delta\rho\right)a_{2}\dot{\theta}_{*}+\left(\dot{\theta}_{*}\right)A_{3}\left(\delta\zeta\right)+\left(\delta\dot{\theta}\right)A_{3}\zeta_{*}+-\frac{r}{\rho}\left(\zeta_{\rho*}\right)A_{4}\left(\delta\zeta\right)-\frac{r}{\rho}\delta\zeta_{\rho}A_{4}\zeta_{*}+\left[a_{5}+\rho_{*}a_{6}\right]\ddot{\theta}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\dot{\zeta}=\left[2\dot{\theta}_{*}\rho_{*}a_{2}+A_{3}\zeta_{*}\right]\delta\dot{\theta}+\left[a_{2}\dot{\theta}_{*}\right]\delta\rho+\left[\dot{\theta}_{*}A_{3}-\frac{r}{\rho_{*}}\zeta_{\rho*}A_{4}-\frac{r}{\rho_{*}}A_{4}\zeta_{*}\mathbf{e}_{x}\right]\delta\zeta+\left[a_{5}+\rho_{*}a_{6}\right]\ddot{\theta}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $A=\left(\begin{array}{ccc}
0 & 0 & 0\\
0 & 0 & r\cos\alpha\mathbf{e}_{y}^{T}\\
2\dot{\theta}_{*}\rho_{*}a_{2}+A_{3}\zeta_{*} & a_{2}\dot{\theta}_{*} & \dot{\theta}_{*}A_{3}-\frac{r}{\rho_{*}}\zeta_{\rho*}A_{4}-\frac{r}{\rho_{*}}A_{4}\zeta_{*}\mathbf{e}_{x}
