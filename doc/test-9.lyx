\begin_inset Formula 
\begin{align*}
p_{b} & =R_{bw}p_{w}\\
\dot{p}_{b} & =\left(R_{wb}\omega_{b,wb}^{\wedge}\right)^{T}p_{w}+R_{bw}\dot{p}_{w}\\
 & =-\omega_{b,wb}^{\wedge}p_{b}+R_{bw}\dot{p}_{w}\\
\ddot{p}_{b} & =-\dot{\omega}_{b,wb}^{\wedge}p_{b}-\omega_{b,wb}^{\wedge}R_{bw}\dot{p}_{w}-\omega_{b,wb}^{\wedge}\dot{p}_{b}+R_{bw}\ddot{p}_{w}\\
 & =-\left(\dot{\omega}_{b,wb}^{\wedge}+\omega_{b,wb}^{\wedge}\omega_{b,wb}^{\wedge}\right)p_{b}-2\omega_{b,wb}^{\wedge}\dot{p}_{b}+R_{bw}\ddot{p}_{w}
